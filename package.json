{
  "name": "lubejs",
  "version": "2.0.0-beta.1",
  "description": "An easy-to-use nodejs oracledb client",
  "main": "lib/lube.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "npm run build && mocha test",
    "build": "tsc",
    "dep": "npm i oracledb mssql --no-save",
    "lint": "eslint --fix ./src --ext .ts",
    "doc": "typedoc --plugin typedoc-plugin-markdown --out doc src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovercao/lubejs.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "oracle",
    "oracledb",
    "mongodb",
    "db",
    "mssql",
    "mysql",
    "database"
  ],
  "author": "jover",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jovercao/lubejs/issues"
  },
  "homepage": "https://github.com/jovercao/lubejs#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.156",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^6.2.1",
    "mockjs": "^1.0.1-beta3",
    "mssql": "^6.2.0",
    "oracledb": "^5.0.0",
    "power-assert": "^1.6.1",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
